  <Import Condition="$(RootNamespace.EndsWith('_km'))" Project="$(SysInformRoot)/Common.Kernel.props" />
  <Import Condition="$(RootNamespace.EndsWith('_um'))" Project="$(SysInformRoot)/Common.User.props" />
</Project>
